<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>
        @ViewData["Title"] - TCV - TraiCayViet
    </title>
    <link rel="stylesheet" href="~/vendor/bootstrap3/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/vendor/font-awesome/css/all.min.css" />
    <link rel="stylesheet" href="~/vendor/animate/animate.min.css">
    <environment include="Development">
        <link rel="stylesheet" href="~/css/homeapp.css" />
        <link rel="stylesheet" href="~/css/animations.css" />
        <link rel="stylesheet" href="~/css/custom.css" />
        <link rel="stylesheet" href="~/css/skins/red.css" />
    </environment>
    <environment exclude="Development">
        <link rel="stylesheet" href="~/css/homeapp.min.css" />
    </environment>
    <script src="~/js/modernizr.js"></script>
    @RenderSection("Styles", false)
</head>
<body>
    <div class="scrollToTop">
        <i class="fas fa-arrow-up"></i>
    </div>
    <partial name="_PartialTopBar" />
    <partial name="_PartialModals" />
    <div class="page-wrapper">
        <partial name="_PartialMenuBar" />
        <div class="main-container">
            <div class="container" style="margin-top: 20px">
                <div class="row">
                    <div class="main object-non-visible" data-animation-effect="fadeInDownSmall" data-effect-delay="300">
                        @RenderBody()
                    </div>
                </div>
            </div>
        </div>
    </div>
    <partial name="_PartialFooter" />
    <script src="~/vendor/jquery/jquery.min.js"></script>
    <script src="~/vendor/bootstrap3/js/bootstrap.min.js"></script>
    <script src="~/vendor/jquery.appear/jquery.appear.min.js"></script>
    <environment include="Development">
        <script src="~/js/homeapp.js" asp-append-version="true"></script>
    </environment>
    <environment exclude="Development">
        <script src="~/js/homeapp.min.js" asp-append-version="true"></script>
    </environment>
    @RenderSection("Scripts", required: false)
</body>
</html>
